{
  "version": 2,
  "builds": [
    {
      "src": "dist/lib-pioneer-scm/server/server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/lib-pioneer-scm/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/lib-pioneer-scm/browser/assets/$1"
    },
    {
      "src": "/(.*).(js|css|png|jpg|ico|svg|json)",
      "dest": "/dist/lib-pioneer-scm/browser/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/lib-pioneer-scm/server/server.mjs"
    }
  ]
}
