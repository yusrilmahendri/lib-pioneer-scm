{
  "functions": {
    "dist/lib-pioneer-scm/server/server.mjs": {
      "runtime": "@vercel/node@20.x" 
    }
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "routes": [
    {
      "src": "/(assets|manifest.webmanifest|ngsw-worker.js|safety-worker.js|worker-basic.min.js|favicon.ico|.*\\.js|.*\\.css|.*\\.png|.*\\.jpg|.*\\.svg)$",
      "dest": "dist/lib-pioneer-scm/browser/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/lib-pioneer-scm/server/server.mjs"
    }
  ]
}